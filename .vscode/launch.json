{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run reactapplication-reactmodulename",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/reactmodulename/index.html"
      ],
      "cwd": "${workspaceFolder}/reactmodulename/build",
      "env": {
        "PORT": "6004",
        "run.config": "{\"handlerId\":\"html5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/reactapplication/reactmodulename\"}"
      },
      "envFile": "${workspaceFolder}/reactmodulename/.env1",
      "preLaunchTask": "Run reactapplication"
    }
  ]
}